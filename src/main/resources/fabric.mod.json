{
  "schemaVersion": 1,
  "id": "cc-fabric-energy",
  "version": "${version}",
  "name": "ComputerCraft Fabric Energy",
  "description": "An interface for ComputerCraft peripherals to interact with modded blocks using Team Reborn's energy API",
  "authors": ["LittleChaSiu"],
  "license": "MIT",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.littlechasiu.ccfe.CCFabricEnergyModKt::init"
      }
    ]
  },
  "depends": {
    "computercraft": ">=1.101.2",
    "team_reborn_energy": ">=2.0.0",
    "fabricloader": ">=0.14.14",
    "fabric-language-kotlin": ">=1.9.1+kotlin.1.8.10",
    "minecraft": ">=1.19.2",
    "java": ">=17"
  }
}
